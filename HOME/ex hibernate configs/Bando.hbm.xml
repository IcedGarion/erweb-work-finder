<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="it.erweb.crawler.model.Bando" table="BANDO">
      <id name="cdBando" type="long" column="CD_BANDO">
         <generator class="native"/>
      </id>
      <property name="cdEsterno" column="CD_ESTERNO" type="string"/>
      <property name="Pubblicazione" column="CD_PUBBLICAZIONE" type="long"/>
      <property name="cig" column="CIG" type="string"/>
	  <property name="tipo" column="TIPO" type="string"/>
	  <property name="tiporichiedente" column="TIPORICHIEDENTE" type="string"/>
	  <property name="nmRichiedente" column="NM_RICHIEDENTE" type="string"/>
	  <property name="scadenza" column="SCADENZA" type="date"/>
	  <property name="oggetto" column="OGGETTO" type="string"/>
	  <property name="testo" column="TESTO" type="string"/>
	  <property name="url" column="URL" type="string"/>
	  <property name="stato" column="STATO" type="string"/>
	  <property name="dtInserimento" column="DT_INSERIMENTO" type="date"/> 
   </class>
</hibernate-mapping>